{
    "name": "lmsui",
    "version": "1.0.0",
    "private": true,
    "description": "LMS UI Project",
    "scripts": {
        "ng": "ng",
        "start": "ng run lmsui:server",
        "test": "ng test",
        "lint": "tslint ./src/**/*.ts -t verbose",
        "e2e": "ng e2e",
        "build-prod": "npm run build:client-and-server-bundles && npm run webpack:server",
        "build-dev": "npm run build:dev-client-and-server-bundles && npm run webpack:server && npm run serve-node-dev",
        "serve-node-dev": "node dist/server.js dev > log.log",
        "serve-node-prod": "node dist/server.js prod ",
        "build:client-and-server-bundles": "node --max_old_space_size=10192 ./node_modules/@angular/cli/bin/ng build --prod && ng run lmsui:server",
        "build:dev-client-and-server-bundles": "node --max_old_space_size=10192 ./node_modules/@angular/cli/bin/ng build && ng run lmsui:server",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "node-only": "npm run webpack:server && npm run serve-node-dev",
        "ang-only": "npm run ng build"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@angular/animations": "^6.1.9",
        "@angular/cdk": "^6.4.7",
        "@angular/common": "^6.1.9",
        "@angular/compiler": "^6.1.9",
        "@angular/core": "^6.1.9",
        "@angular/flex-layout": "^6.0.0-beta.18",
        "@angular/forms": "^6.1.9",
        "@angular/http": "^6.1.9",
        "@angular/material": "^6.4.7",
        "@angular/material-moment-adapter": "^8.2.3",
        "@angular/platform-browser": "^6.1.9",
        "@angular/platform-browser-dynamic": "^6.1.9",
        "@angular/router": "^6.1.9",
        "@angular/upgrade": "^7.1.0",
        "@fortawesome/fontawesome-free": "^5.12.1",
        "@nguniversal/express-engine": "^7.1.0",
        "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
        "chart.js": "^2.7.1",
        "compression": "^1.7.3",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "material-design-icons": "^3.0.1",
        "moment": "^2.22.2",
        "npm": "^6.4.1",
        "popper.js": "^1.12.9",
        "request": "^2.81.0",
        "rxjs": "^6.3.3",
        "rxjs-compat": "^6.0.0-rc.0",
        "xlsx": "^0.12.12",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.0",
        "@angular/cli": "^6.2.3",
        "@angular/compiler-cli": "^6.1.9",
        "@angular/language-service": "^6.1.9",
        "@angular/platform-server": "^7.1.0",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "lodash": "^4.16.2",
        "protractor": "^5.4.1",
        "ts-loader": "^6.0.4",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.9.2",
        "webpack-cli": "^3.3.12"
    },
    "repository": {}
